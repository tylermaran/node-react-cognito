{
  "name": "node-react-cognito",
  "version": "1.0.0",
  "description": "reusable secure signin template using node, react, and AWS Cognito",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "client": "cd client && npm run start",
		"client_prod": "cd client && npm run prod",
		"server": "cd server && nodemon server.js",
		"dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
		"prod": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client_prod\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tylermaran/node-react-cognito.git"
  },
  "keywords": [
    "react",
    "node",
    "aws",
    "cognito"
  ],
  "author": "themanmaran (tyler.maran@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tylermaran/node-react-cognito/issues"
  },
  "homepage": "https://github.com/tylermaran/node-react-cognito#readme",
  "dependencies": {
    "concurrently": "^5.0.0"
  }
}
